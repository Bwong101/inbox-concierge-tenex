{
  "name": "inbox-concierge-tenex",
  "version": "1.0.0",
  "description": "Gmail inbox management toolkit with CLI and web interface",
  "main": "src/cli.js",
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "cli": "node src/cli.js analyze-filter",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "cd client && npx vite",
    "build": "cd client && npm install && npx vite build",
    "continuous": "node src/cli.js continuous",
    "cleanup": "node src/cli.js cleanup",
    "cleanup-apply": "node src/cli.js cleanup --apply",
    "spam-rescue": "node src/cli.js spam-rescue",
    "trash-by-sender": "node src/cli.js trash-by-sender",
    "reset": "rm -f .checkpoint.json && echo 'Checkpoint reset!'",
    "test": "node --test 'test/*.test.js'"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "googleapis": "^144.0.0",
    "dotenv": "^16.4.5",
    "chalk": "^5.3.0",
    "ora": "^8.1.0",
    "express": "^4.21.0",
    "express-session": "^1.18.0",
    "memorystore": "^1.6.7",
    "cors": "^2.8.5",
    "@anthropic-ai/sdk": "^0.39.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}
